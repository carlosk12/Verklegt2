@model  StalkBook.Models.GroupViewModel

@{
    ViewBag.Title = "View";
}

<div>
    <h2>Groups</h2>

    <div class="newsfeed-box">
        <form role="form" method="post">
            <div class="form-group status-content">
                @Html.TextBoxFor(model => model.name, new { @placeholder = "Enter group name", })
            </div>
            <button type="submit" class="btn btn-default">Create Group</button>
        </form>
    </div>

    <div class="search-profiles">
        <ul>
        @foreach (var item in Model.groups)
        {
            <li>
                @item.name
            </li>
        }
        </ul>
    </div>
</div>